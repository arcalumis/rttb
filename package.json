{
	"name": "arcalum.is/rtt4b",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "concurrently \"bun run dev:server\" \"bun run dev:client\"",
		"dev:host": "concurrently \"bun run dev:server\" \"bun run dev:client -- --host\"",
		"dev:client": "vite",
		"dev:server": "bun run --watch server/index.ts",
		"server": "bun run server/index.ts",
		"build": "tsc -b && vite build",
		"lint": "biome lint .",
		"format": "biome format . --write",
		"check": "biome check .",
		"check:types": "tsc --noEmit",
		"check:lint": "biome lint .",
		"check:format": "biome check --formatter-enabled=true .",
		"preview": "vite preview"
	},
	"dependencies": {
		"@fastify/cors": "^11.2.0",
		"@fastify/helmet": "^13.0.2",
		"@fastify/multipart": "^9.3.0",
		"@fastify/rate-limit": "^10.3.0",
		"@fastify/static": "^9.0.0",
		"@tailwindcss/vite": "^4.1.4",
		"bcryptjs": "^3.0.3",
		"dotenv": "^17.2.3",
		"fastify": "^5.6.2",
		"fastify-raw-body": "^5.0.0",
		"heic2any": "^0.0.4",
		"jsonwebtoken": "^9.0.3",
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"react-router-dom": "^7.12.0",
		"replicate": "^1.4.0",
		"resend": "^6.7.0",
		"sharp": "^0.34.5",
		"stripe": "^20.1.2",
		"tailwindcss": "^4.1.4"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"@types/bcryptjs": "^3.0.0",
		"@types/jsonwebtoken": "^9.0.10",
		"@types/react": "^19.0.10",
		"@types/react-dom": "^19.0.4",
		"@vitejs/plugin-react-swc": "^3.8.0",
		"concurrently": "^9.2.1",
		"typescript": "~5.7.2",
		"vite": "^6.3.1"
	}
}
